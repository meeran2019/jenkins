pipeline {
  agent any
  stages {
    stage('Buzz Buzz') {
      steps {
        echo 'Bees Buzz!'
      }
    }

    stage('Bees Bees') {
      steps {
        echo 'Buzz, Bees, Buzz!'
        echo 'Bees Buzzing!'
        echo 'Bees Buzzing Again'
      }
    }

    stage('assignment-stage') {
      steps {
        echo 'assignment step'
      }
    }

    stage('feature-branch-step') {
      steps {
        echo 'feature branch'
      }
    }

  }
}